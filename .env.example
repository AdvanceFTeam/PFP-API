# Discord bot token (required)
DISCORD_BOT_TOKEN=<your-discord-bot-token>

# GitHub personal access token (required)
GITHUB_TOKEN=<your-github-token>

# Port your server will run on (default: 3000)
PORT=3000

# Cache Time-To-Live in seconds (default: 60)
CACHE_TTL=60

# Supabase project URL (required if you are using Supabase)
SUPABASE_URL=https://<your-project-ref>.supabase.co

# Supabase service role key (required if you are using Supabase server-side functions)
SUPABASE_SERVICE_ROLE_KEY=<your-service-role-key>
